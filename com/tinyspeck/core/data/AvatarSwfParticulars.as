	import com.tinyspeck.engine.util.EnvironmentUtil;
	
	public class AvatarSwfParticulars {
		
		// hat_ob and hair_ob can be either generic objects or AvatarConfigArticles
		public static function hatCanBeWornWithHair(hat_ob:Object, hair_ob:Object):Boolean {
			if (EnvironmentUtil.getUrlArgValue('SWF_hat_and_hair_all') == '1') return true;
			if (!hat_ob) return true;
			if (!hair_ob) return true;
			if (hair_ob.article_class_name == '') return true;
			if (hair_ob.article_class_name == 'none') return true;
			if (!hat_hair_map[hat_ob.article_class_name]) return false;
			if (!hat_hair_map[hat_ob.article_class_name] is Array) return false;
			
			var ok_hairA:Array = hat_hair_map[hat_ob.article_class_name] as Array;
			if (ok_hairA.indexOf(hair_ob.article_class_name) > -1) {
				return true;
			}
			
			return false;
		}
		
