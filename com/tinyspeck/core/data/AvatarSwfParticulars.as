		]
		
		// coat_ob can be either generic object or AvatarConfigArticle
		public static function coatHidesAllOtherSleeves(coat_ob:Object):Boolean {
			if (EnvironmentUtil.getUrlArgValue('SWF_hide_all_sleeves') == '1') return true;
			if (!coat_ob) return false;
			if (coat_ob.article_class_name == 'none' || coat_ob.article_class_name == '') return false;
			
			// if it is not in the coats_that_allow_sleeves array, it hides all sleeves
			return (coats_that_allow_sleeves.indexOf(coat_ob.article_class_name) == -1);
		}
		
